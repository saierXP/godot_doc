#: ../../docs/classes/class_scenetree.rst:250
msgid "enum **GroupCallFlags**:"
msgstr ""

#: ../../docs/classes/class_scenetree.rst:252
msgid "**GROUP_CALL_DEFAULT** = **0** --- Call a group with no flags (default)."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:254
msgid "**GROUP_CALL_REVERSE** = **1** --- Call a group in reverse scene order."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:256
msgid "**GROUP_CALL_REALTIME** = **2** --- Call a group immediately (calls are normally made on idle)."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:258
msgid "**GROUP_CALL_UNIQUE** = **4** --- Call a group only once even if the call is executed many times."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:270
msgid "enum **StretchMode**:"
msgstr ""

#: ../../docs/classes/class_scenetree.rst:272
msgid "**STRETCH_MODE_DISABLED** = **0** --- No stretching."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:274
msgid "**STRETCH_MODE_2D** = **1** --- Render stretching in higher resolution (interpolated)."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:276
msgid "**STRETCH_MODE_VIEWPORT** = **2** --- Keep the specified display resolution. No interpolation. Content may appear pixelated."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:292
msgid "enum **StretchAspect**:"
msgstr ""

#: ../../docs/classes/class_scenetree.rst:294
msgid "**STRETCH_ASPECT_IGNORE** = **0** --- Fill the window with the content stretched to cover excessive space. Content may appear stretched."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:296
msgid "**STRETCH_ASPECT_KEEP** = **1** --- Retain the same aspect ratio by padding with black bars on either axis. This prevents distortion."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:298
msgid "**STRETCH_ASPECT_KEEP_WIDTH** = **2** --- Expand vertically. Left/right black bars may appear if the window is too wide."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:300
msgid "**STRETCH_ASPECT_KEEP_HEIGHT** = **3** --- Expand horizontally. Top/bottom black bars may appear if the window is too tall."
msgstr ""

#: ../../docs/classes/class_scenetree.rst:302
msgid "**STRETCH_ASPECT_EXPAND** = **4** --- Expand in both directions, retaining the same aspect ratio. This prevents distortion while avoiding black bars."
msgstr ""

